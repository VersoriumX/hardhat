{
    // Use IntelliSense to learn about possible attributes.
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Attach to Process",
            "type": "go",
            "request": "attach api",
            "mode": "local",
            "processId": 0
        },

        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },
        {
            "name": "Attach to Edge",
            "port": 8334,
            "request": "attach",
            "type": "msedge",
            "webRoot": "${VersoriumX}"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                     "<https://versoriumx5.infura-ipfs.io>"          ],
            "program": "${VersoriumX}\\src\\internal\\provider-proxy.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${VersoriumX}/build-test/**/*.js"
            ]
        }

    ]
}